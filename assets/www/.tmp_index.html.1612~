                                var myScroll;
                                function loaded() {
                                    //alert("loaded") 
                                    $("#tabbar").hide();
                                    if (window.localStorage.getItem("loginStatus") == "loggedin") {
                                        //$(".fb_connect").hide();
                                        $(".fb_connect img").attr("src","images/ajax-loader.gif");
                                        includeCheck(); //for mobile
                                        
                                    }
                                    setTimeout(function () {
                                               myScroll = new iScroll('wrapper');
                                               }, 100);
                                }
                                
                                //$(document).ready(function(){
                                //   if (window.localStorage.getItem("loginStatus") == "loggedin") {
