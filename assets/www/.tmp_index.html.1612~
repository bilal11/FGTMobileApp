                                var mydata = '';
                                
                                // Main functions:
                                $(function() {
                                  
                                  
                                  $('div').bind('pageAnimationEnd', function(e, info) {
                                                //console.log("page animation end   id:" +e.target.id + "    class:" + e.target.class)
                                                
                                                if (myScroll) {
                                                myScroll.destroy();
                                                myScroll = null;
                                                //alert('destroy iscroll')
                                                }
                                                //alert(e.target.id)
                                                if ($('div#' + e.target.id + ' #wrapper').get(0)) {
                                                setTimeout(function() {
                                                           myScroll = new iScroll($('div#' + e.target.id + ' #wrapper').get(0));
                                                           $("#tabbar").css("z-index","9999");
                                                           //alert('New iscroll');
                                                           }, 1000);
                                                
                                                }
                                                e.preventDefault();
                                                return false;
                                                
                                                //alert('New iscroll');
                                                }); //pageAnimationEnd
                                  
                                  
                                  
                                  }); //end all function()
                                document.addEventListener("deviceready", onDeviceReady, true);
                                
                                function onDeviceReady(){
                                    
                                    document.addEventListener("pause", pauseEvent, false);
                                    document.addEventListener("resume", resumeEvent, false);
                                    
                                    $("#tabbar").hide();
                                    if (window.localStorage.getItem("loginStatus") == "loggedin") {
                                        //$(".fb_connect").hide();
                                        $(".fb_connect img").attr("src","images/ajax-loader.gif");
                                        includeCheck(); //for mobile
                                        
                                    }
                                    
                                }
                                function pauseEvent(e){
                                    //alert("app paused");
                                    //$(".flip").removeClass("current")
                                }
                                
                                
                                function resumeEvent(e){
                                    //$("#home").trigger("click")
                                    
                                    //$(".home_footer").addClass("current")
                                    //includeCheck();
                                    if (window.localStorage.getItem("loginStatus") == "loggedin") {
                                        
                                        //location.hash = "#home"
                                    }
                                    
                                    //alert("app resumed");
                                }
                                
                                
                                $('.fb_connect').live('click',function(e){
                                                      //alert("d r")
                                                      $(".fb_connect img").attr("src","images/ajax-loader.gif")
                                                      var my_client_id = "410323929009950", my_redirect_uri = "https://www.facebook.com/connect/login_success.html", my_type = "user_agent", my_display = "touch";
                                                      var authorize_url = "https://graph.facebook.com/oauth/authorize?";
                                                      authorize_url += "client_id=" + my_client_id;
                                                      authorize_url += "&redirect_uri=" + my_redirect_uri;
                                                      authorize_url += "&display=" + my_display;
                                                      authorize_url += "&scope=read_stream,publish_stream,offline_access,publish_checkins"
                                                      //alert("here")
                                                      
                                                      if(window.plugins.childBrowser == null)
                                                      {
                                                      ChildBrowser.install();
                                                      }
                                                      //var cb  
                                                      window.plugins.childBrowser.onLocationChange = onCBLocChange;
                                                      
                                                      window.plugins.childBrowser.showWebPage(authorize_url);
                                                      return false;
                                                      //$(this).hide();
                                                });
                                function onCBLocChange(loc){
                                    //alert("cb chng")
                                    if (loc.indexOf("https://www.facebook.com/connect/login_success.html") > -1) {
                                        var fbCode = loc.match(/code=(.*)$/)[1]
                                     
                                                $.support.cors = true;
                                        $.ajax({
                                               url: 'https://graph.facebook.com/oauth/access_token?client_id=410323929009950&client_secret=6bdf1d312a3187fee5af8163b25a3fc4&redirect_uri=https://www.facebook.com/connect/login_success.html&code=' + fbCode,
                                               data: {},
                                               success: function(data, status){
                                               //alert(data.split("=")[1])
                                               //alert(data.split("=")[1].split(""))
                                               window.localStorage.setItem("access_token", data.split("=")[1].split("&")[0]);
                                               window.localStorage.setItem("facebook_token", data.split("=")[1].split("&")[0]);
                                               window.plugins.childBrowser.close();
                                               //                alert("in success");
                                               initialize_cb_facebook();
                                               },
                                               error: function(error){
                                               //                alert(JSON.stringify(error));
                                               window.plugins.childBrowser.close();
                                               },
                                               dataType: 'text',
                                               type: 'POST'
                                               });
                                    }

                                    
                                }
                                function initialize_cb_facebook(){
                                    var url = "https://graph.facebook.com/me?access_token=" + window.localStorage.getItem('access_token');
                                    //alert(url)
                                    $.getJSON(url, function(data){
                                              window.localStorage.setItem("facebook_id", data.id);
                                              window.localStorage.setItem("user_name",data.name);
                                              window.localStorage.setItem("loginStatus", "loggedin");
                                              //        includeCheck();
                                              var command="http://nightlifeapp.hoosierballs.com/facebook/connect.json?facebook_id="+window.localStorage.getItem("facebook_id")+"&access_token="+window.localStorage.getItem("access_token");
                                              //alert(command);
                                              $.getJSON(command, function(datam){
                                                        if(datam){
                                                        //alert(JSON.stringify(datam));
                                                        //location.hash="#home";
                                                       
                                                        includeCheck();		
                                                        }
                                                        });
                                              });
                                }

